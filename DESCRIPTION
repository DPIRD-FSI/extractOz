Package: extractOz
Title: Extract Australian Soil, Weather and GRDC Agro-ecological Zone
        Information from GPS Points
Version: 0.0.2.9000
Authors@R: c(person(given = "Adam H.", family = "Sparks",
                     email = "adam.sparks@dpird.wa.gov.au",
                     role = c("aut", "cre"),
                     comment = c(ORCID = "0000-0002-0061-8359")),
             person(given = "Western Australia Department of Primary Industries
                    and Regional Development", role = "cph",
